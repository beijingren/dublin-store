Prefix(:=<http://example.org/owl/sikuquanshu#>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(owl:=<http://www.w3.org/2002/07/owl#>)

Ontology(<http://example.org/owl/sikuquanshu#>

	Declaration(Class(:Person))
	Declaration(Class(:City))
	Declaration(Class(:Settlement))
	Declaration(Class(:PopulatedPlace))
	Declaration(Class(:Place))
	Declaration(Class(:Xian))
	Declaration(Class(:SexagenaryCycle))
	Declaration(Class(:OfficialTitle))

	SubClassOf(:Person owl:Thing)
	SubClassOf(:Place owl:Thing)
	SubClassOf(:PopulatedPlace :Place)
	SubClassOf(:Settlement :PopulatedPlace)
	SubClassOf(:City :Settlement)

	Declaration(ObjectProperty(:zi))
	Declaration(ObjectProperty(:hao))
	Declaration(ObjectProperty(:shihao))
	Declaration(DataProperty(:jinshi))

	Declaration(ObjectProperty(:hasChild))
	Declaration(ObjectProperty(:hasWife))

	Declaration(NamedIndividual(:曹操))
	ClassAssertion(:Person :曹操)
	ObjectPropertyAssertion(:zi :曹操 :孟德)
	ObjectPropertyAssertion(:shihao :曹操 :武王)

	DataPropertyAssertion(:birthYear :曹操 "155"^^xsd:gYear)
	DataPropertyAssertion(:deathYear :曹操 "220"^^xsd:gYear)
	ObjectPropertyAssertion(:birthPlace :曹操 :譙縣)
	ObjectPropertyAssertion(:deathPlace :曹操 :洛陽)
	ObjectPropertyAssertion(:hasChild :曹操 :曹丕)
	ObjectPropertyAssertion(:hasChild :曹操 :曹彰)
	ObjectPropertyAssertion(:hasChild :曹操 :曹植)
	ObjectPropertyAssertion(:hasChild :曹操 :曹熊)
	ObjectPropertyAssertion(:hasWife :曹操 :卞氏)

	Declaration(NamedIndividual(:曹丕))
	ClassAssertion(:Emperor :曹丕)
	ObjectPropertyAssertion(:zi :曹丕 :子桓)
	ObjectPropertyAssertion(:shihao :曹丕 :文皇帝)

	DataPropertyAssertion(:birthYear :曹丕 "187"^^xsd:gYear)
	DataPropertyAssertion(:deathYear :曹丕 "226"^^xsd:gYear)
	ObjectPropertyAssertion(:hasChild :曹丕 :曹睿)

	Declaration(NamedIndividual(:曹熊))
	ClassAssertion(:Person :曹熊)
	ObjectPropertyAssertion(:hasChild :曹熊 :曹炳)

	Declaration(NamedIndividual(:徐宣))
	ClassAssertion(:Person :徐宣)

	DataPropertyAssertion(:deathYear :徐宣 "236"^^xsd:gYear)
	ObjectPropertyAssertion(:zi :徐宣 :寶堅)
	ObjectPropertyAssertion(:birthPlace :徐宣 :海西)

	Declaration(NamedIndividual(:徐準))
	ClassAssertion(:Person :徐準)

	ObjectPropertyAssertion(:zi :徐準 :慕萊)
	ObjectPropertyAssertion(:hao :徐準 :立亭)

	ObjectPropertyAssertion(:birthPlace :徐準 :遵義)

	ClassAssertion(:Person :韓愈)
	DataPropertyAssertion(:birthYear :韓愈 "768"^^xsd:gYear)
	DataPropertyAssertion(:deathYear :韓愈 "824"^^xsd:gYear)
	ObjectPropertyAssertion(:zi :韓愈 :退之)
	ObjectPropertyAssertion(:shihao :韓愈 :文)

	ClassAssertion(:Person :柳開)

	ClassAssertion(:Person :蘇舜元)
	DataPropertyAssertion(:birthYear :蘇舜元 "1006"^^xsd:gYear)
	DataPropertyAssertion(:deathYear :蘇舜元 "1054"^^xsd:gYear)

	ClassAssertion(:Person :蘇舜欽)
	DataPropertyAssertion(:birthYear :蘇舜欽 "1008"^^xsd:gYear)
	DataPropertyAssertion(:deathYear :蘇舜欽 "1048"^^xsd:gYear)
	DataPropertyAssertion(:jinshi :蘇舜欽 "1035"^^xsd:gYear)
	ObjectPropertyAssertion(:zi :蘇舜欽 :子美)

	ClassAssertion(:Person :歐陽修)
	DataPropertyAssertion(:birthYear :歐陽修 "1007"^^xsd:gYear)
	DataPropertyAssertion(:deathYear :歐陽修 "1072"^^xsd:gYear)
	ObjectPropertyAssertion(:zi :歐陽修 :永叔)
	ObjectPropertyAssertion(:hao :歐陽修 :醉翁)
	ObjectPropertyAssertion(:hao :歐陽修 :六一居士)
	ObjectPropertyAssertion(:shihao :歐陽修 :文忠)

	ClassAssertion(:Person :穆修輩)

	ClassAssertion(:Person :曾覿)
	DataPropertyAssertion(:birthYear :曾覿 "1109"^^xsd:gYear)
	DataPropertyAssertion(:deathYear :曾覿 "1180"^^xsd:gYear)
	ObjectPropertyAssertion(:zi :曾覿 :純甫)
	ObjectPropertyAssertion(:shihao :曾覿 :海野老農)

	ClassAssertion(:City :洛陽)

	ClassAssertion(:Xian :廬陵)

	ClassAssertion(:SexagenaryCycle :庚子)

	ClassAssertion(:OfficialTitle :諫議大夫)
)
